[banana]
# Required for real Nano Banana calls
api_key = "your-nano-banana-api-key"
base_url = "https://api.nano-banana.ai"
free_model = "nano-banana"  # regular model (default)
# Uncomment to add an optional Pro override/fallback:
# pro_model = "nano-banana-pro"
# Flip to true to use on-device mock extraction instead of the API
mock_mode = false
